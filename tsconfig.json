{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
        "DOM",
        "DOM.Iterable",
        "ESNext"
    ],
    "jsx": "preserve",
    "module": "ESNext",
    "rootDir": "./src",
    "moduleResolution": "node",
    "baseUrl": "./src",
    "resolveJsonModule": true,
    "allowJs": true,
    "noEmit": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  },
  "include": [
      "./src"
  ]
}
